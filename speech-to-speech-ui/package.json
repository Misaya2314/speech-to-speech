{
  "name": "speech-to-speech-ui",
  "version": "1.0.0",
  "description": "语音对话应用桌面界面",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "electron:dev": "cross-env NODE_ENV=development electron .",
    "electron:build": "vue-tsc --noEmit && vite build && electron-builder",
    "start": "concurrently npm:dev npm:electron:dev",
    "preview": "vite preview",
    "reinstall": "rimraf node_modules && npm cache clean --force && npm install",
    "clean": "rimraf dist node_modules .vite"
  },
  "keywords": [
    "speech-to-speech",
    "electron",
    "vue",
    "AI",
    "TTS",
    "STT"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@vitejs/plugin-vue": "^4.0.0",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "electron": "^22.3.27",
    "electron-builder": "^23.6.0",
    "rimraf": "^5.0.0",
    "sass": "^1.56.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vue-tsc": "^1.0.9"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "axios": "^1.2.0",
    "element-plus": "^2.9.8",
    "pinia": "^2.0.27",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "build": {
    "appId": "com.speechtospeech.app",
    "productName": "Speech To Speech",
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "directories": {
      "output": "release"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "public/icon.ico"
    }
  }
}